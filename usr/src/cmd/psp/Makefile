UTSBASE =	$(SRC)/uts

PROG =		pspeinj

include ../Makefile.cmd
include ../Makefile.cmd.64
include ../Makefile.ctf

ROOTCMDDIR =	$(ROOTLIB)/psp

CSTD =		$(CSTD_GNU17)
CTF_MODE =	link

CPPFLAGS +=	-I$(UTSBASE)/intel/io/amdzen -I$(UTSBASE)/oxide/io/psp
CFLAGS +=	$(CCVERBOSE)

.KEEP_STATE:

all: $(PROG)

install: all $(ROOTCMD)

clean:
	$(RM) $(OBJS)

include ../Makefile.targ
